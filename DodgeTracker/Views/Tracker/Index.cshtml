<h1>Riot API</h1>

<form method="post" asp-controller="Tracker" asp-action="Index">
    <label for="tagLine">TagLine:</label><br />
    <input type="text" id="tagLine" name="tagLine"><br />
    <label for="gameName">Game Name:</label><br />
    <input type="text" id="gameName" name="gameName"><br />    
    <button type="submit" class="btn btn-primary">Send</button>
</form>

@if (ViewBag.Result != null)
{
    <pre>@ViewBag.Result</pre>
}